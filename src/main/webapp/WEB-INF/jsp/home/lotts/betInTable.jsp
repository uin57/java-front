<%@page contentType="text/html" pageEncoding="UTF-8" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><div class="detailHeaderBox">	<div class="headerDetail">		<p>开启彩种：${betInTotal.lotteryName }</p>		<p>开启玩法：${betInTotal.playName }</p>		<p>开启时间：<fmt:formatDate value='${betInTotal.startTime}' pattern="yyyy-MM-dd HH:mm:ss"/></p>		<p>启动金额：${betInTotal.startAmount }元</p>		<p>游戏编号：${betInTotal.betId }</p>	</div></div><div class="detailBody">	<table class="userListTable overflowTable" id="betInListTable">		<tr class="listHeader">			<th width="20%">游戏</th>			<th width="20%">游戏金额</th>			<th width="20%">倍数</th>			<th width="20%">获得金额</th>			<th width="20%">盈利</th>		</tr>		<c:forEach items="${betInList }" var="betIn" varStatus="st">			<tr class="listDetail">				<td>${st.index + p.startIndex + 1 }</td>				<td>${betIn.amount }</td>				<td>${betIn.multiples }</td>				<td>${betIn.win }</td>				<td>${betIn.win - betIn.amount }</td>			</tr>		</c:forEach>	</table></div><div class="dialogBtn">	<a href="javascript:;" class="btn closeBtn">确定</a></div>