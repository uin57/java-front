<%@page contentType="text/html" pageEncoding="UTF-8" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><input type="hidden" id="pageCountBetIn" value="${p.rowCount }"/><table class="userListTable" id="betInListTable">	<tr class="listHeader">		<th width="5%">序号</th>		<th width="10%">账号</th>		<th width="10%">开启彩种</th>		<th width="10%">开启玩法</th>		<th width="10%">启动金额</th>		<th width="15%">开启时间</th>		<th width="10%">游戏次数</th>		<th width="10%">游戏金额</th>		<th width="10%">中奖金额</th>		<th width="10%">操作</th>	</tr>	<c:forEach items="${betInTotalList }" var="betInTotal" varStatus="st">		<tr class="listDetail">			<td>${st.index + p.startIndex + 1 }</td>			<td>${betInTotal.account }</td>			<td>${betInTotal.lotteryName }</td>			<td>${betInTotal.playName }</td>			<td>${betInTotal.startAmount }</td>			<td><fmt:formatDate value='${betInTotal.startTime}' pattern="yyyy-MM-dd HH:mm:ss"/></td>			<td>${betInTotal.gameCount }</td>			<td>${betInTotal.gameAmount }</td>			<td>${betInTotal.winAmount }</td>			<td>				<a href="javascript:;" class="fontColorTheme" onclick="showBetInDetail('${betInTotal.id}')">查看详情</a>			</td>		</tr>	</c:forEach></table><c:if test="${betInTotalList == null || betInTotalList.size() == 0 }">	<!-- 没有消息 -->	<div class="nullMsg" id="nullMsg">		<i class="nullMsgImg"></i>		<span class="fontColorGray">没有符合条件的记录，请更改查询条件！</span>	</div></c:if><script type="text/javascript">	function showBetInDetail(id) {		ajaxLoad('betInDivBox', '/lotts/betIn/betInTable', {"id": id}, function () {			layer.open({				type: 1,				skin: 'generalLayer',				shift: 5,				area: '960px',				title: '详情',				content: $('#betInDiv'),				success: function () {					$('.closeBtn').on('click', function () {						layer.closeAll();					});				}			});		});	}	$(function () {		var o = 0, i = 0, a = 0;		// 统计		$("#betInListTable td[id^='tdChangeAmount_']").each(function () {			var m = parseFloat($(this).html());			if (m < 0) {				o = Math.add(o, m);			} else {				i = Math.add(i, m);			}			a = Math.add(a, m);		});		$("#tdOut").html(o);		$("#tdIn").html(i);		$("#tdAll").html(a);	});</script>