<%@page contentType="text/html" pageEncoding="UTF-8" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt" %><%@ taglib uri="http://www.sjc168.com/fn" prefix="fn" %><%@ taglib prefix="fnc" uri="http://java.sun.com/jsp/jstl/functions" %><!DOCTYPE html><html lang="en"><head>	<title>${lott.title}</title>	<script src="<c:url value='/res/home/js/lotts/slotMachine.js?ver=${VIEW_VERSION}'/>"></script>	<link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">	<script src="<c:url value='/res/home/js/math.extends.js'/>"></script>	<script src="<c:url value='/res/home/js/lotts/play.js?ver=${VIEW_VERSION}'/>"></script>	<script src="<c:url value='/res/home/js/lotts/play_${lottBase.groupId}.js?ver=${VIEW_VERSION}'/>"></script>	<script src="<c:url value='/res/home/js/lotts/fire.js?ver=${VIEW_VERSION}'/>"></script>	<script src="<c:url value='/res/home/js/lotts/lotterySlot.js?ver=${VIEW_VERSION}'/>"></script>	<script src="<c:url value='/res/home/js/ajaxfileupload.js?ver=${VIEW_VERSION}'/>"></script></head><body><form action="" id="betConfirmForm"></form><div class="main">	<!--彩种头部-->	<div class="lotteryHeaderBox clr">		<div class="lotteryHeader">			<p class="currLottNumBox">				<span>第</span>				<span>161104058</span>				<span>期开奖结果</span>			</p>			<div class="lotteryNumArea">				<div class="lotteryNumBox">					<ul class="lotteryNum clr ${lott.id}" id="openNums" data-num="${lottBase.numList}">						<c:forEach begin="0" end="4" var="a">							<li class="lottNumDetail">								<c:forEach begin="0" end="9" var="b">									<p><span>${b}</span></p>								</c:forEach>							</li>						</c:forEach>					</ul>				</div>			</div>			<div class="topThree">				<ul id="openedNum">					<c:forEach items="${openList}" var="betTiger">						<li class="openedNumDetail">${betTiger.openNum}</li>					</c:forEach>				</ul>			</div>		</div>	</div>	<div class="lottery">		<!--彩种内容-->		<div class="lotteryContent">			<div class="lotteryTypeArea clr">				<ul class="lottNavBox clr">					<c:forEach items="${lottBase.qun }" var="qun" varStatus="st">						<c:if test="${qun.playerCount>0 }">							<li class="lottNavDetail" data-show="#group_${st.index }" data-hide=".lottTypeBox"><a									href="javascript:;">${qun.title }</a></li>						</c:if>					</c:forEach>				</ul>				<!-- 彩种组 -->				<div class="lottTypeGroup">					<c:forEach items="${lottBase.qun }" var="qun" varStatus="st">						<c:if test="${qun.playerCount>0 }">							<div class="lottTypeBox" id="group_${st.index}" style="display: none;">								<c:forEach items="${qun.groups}" var="group">									<dl class="lottTypeList">										<dt>${group.title}</dt>										<dd>											<c:forEach items="${group.players }" var="player">												<c:set var="p" value="${lottBase.playerBonusMap[player.id] }"/>												<c:if test="${p!=null && p.saleStatus==0 }">													<a href="javascript:;" class="lottTypeDetail"													   data-hide=".changeNumList" data-show="#${p.id }"													   data-remark="${p.remark}"													   data-example="${p.example}" data-title="${player.fullTitle}"													   data-MaxBonus="${fn:getMaxBonus(player.bonusStr,p.bonusRatio,user.rebateRatio,bonusGroup.rebateRatio,p.rebateRatio)}"													   data-bonus="${fn:getMinBonus(player.bonusStr,p.bonusRatio)}">${p.title }</a>												</c:if>											</c:forEach>										</dd>									</dl>								</c:forEach>							</div>						</c:if>					</c:forEach>				</div>			</div>			<!--彩种选号区域-->			<div class="areaContainer">				<!--选号区域-->				<div class="lottChangeNumMain">					<div class="lottChangeNumBox">						<div class="changeNumListBox">							<!--选号列表-->							<c:forEach items="${lottBase.players }" var="player">								<div class="changeNumList changeNumList-${lott.id}" style="display: none;"									 id="${player.id }" data-anySelect="${player.anySelect }">									<c:if test="${player.anyList!=null }">										<div class="anySelectBox">											<c:forEach items="${player.anyList }" var="a" varStatus="stat">												<c:choose>													<c:when test="${player.anySelect>stat.index  }">														<input class="anySelect" type="checkbox" checked="checked"															   value="${a }"/>${a }&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;													</c:when>													<c:otherwise>														<input class="anySelect" type="checkbox"															   value="${a }"/>${a }&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;													</c:otherwise>												</c:choose>											</c:forEach>										</div>									</c:if>									<c:choose>										<c:when test="${player.numView!=null }">											<c:forEach items="${player.numView }" var="line">												<div class="changeNumDetail${line.towLine? ' changeNumDouble':'' }">													<div class="changeNum">														<span class="detailTitle">${line.title }</span>														<ul class="numList" data-max="${line.maxSelect }">															<c:forEach items="${line.nums }" var="n" varStatus="st">																<li><a href="javascript:;" data-num="${n}"																	   class="smallNum${fn:getTitle(n)}">${n}</a></li>															</c:forEach>														</ul>													</div>													<div class="changeNumBtn">														<ul class="btnList">															<c:if test="${line.hasBut }">																<li><a href="javascript:;"																	   class="numBtn numBtnAll">全</a></li>																<li><a href="javascript:;"																	   class="numBtn numBtnBig">大</a></li>																<li><a href="javascript:;"																	   class="numBtn numBtnSmall">小</a></li>																<li><a href="javascript:;"																	   class="numBtn numBtnOdd">单</a></li>																<li><a href="javascript:;"																	   class="numBtn numBtnEven">双</a></li>																<li><a href="javascript:;"																	   class="numBtn numBtnClear">清</a></li>															</c:if>														</ul>													</div>												</div>											</c:forEach>										</c:when>										<c:otherwise>											<!-- 单式 -->											<div class="changeNumSingle">												<div class="importBetList">													<a href="javascript:;" class="btn uploadFile">导入注单</a>												</div>												<div class="singleNumBox">													<textarea class="singleNum"></textarea>													<div class="singleMsg">														<p>说明：</p>														<p>1.号码无需分割。</p>														<p>2.每一注号码之间的间隔符支持回车 空格[ ] 逗号[ , ] 分号[ ; ]</p>														<p>3.文件格式必须是.txt格式</p>														<p>4. 文件较大时会导致上传时间较长，请耐心等待！</p>														<p>5.导入文本内容后将覆盖文本框中现有的内容。</p>													</div>												</div>												<div class="singleBtns">													<a href="javascript:;" class="btn errBtn"													   data-playId="#${player.id }">删除错误项</a>													<a href="javascript:;" class="btn cfBtn"													   data-playId="#${player.id }">删除重复项</a>													<a href="javascript:;" class="btn clearBtn"													   data-playId="#${player.id }">清空文本框</a>													<span class="msg" style="display: none;">您导入的号码有错误项和重复项，已为您标记，试试功能键由系统修正错误</span>												</div>											</div>										</c:otherwise>									</c:choose>								</div>							</c:forEach>							<div id="fileDiv" style="display:none;">								<input type="file" name="file" id="file"/>							</div>						</div>						<!--确认投注-->						<div class="confirmBetsBox">							<div class="betsBtn">								<a href="javascript:;" class="appendNumBtn" id="addSelectNum">确认添加</a>								<a href="javascript:;" class="confirmBets" id="promptlyBet2">开始</a>							</div>							<div class="betsInfo">								<label class="betsInfoTips">									<span>共</span>									<span class="fontColorTheme" id="betsCount">0</span>									<span>注，金额</span>									<span class="fontColorTheme" id="betsAmountCount">0</span>									<span>元</span>								</label>								<label class="amountTypeDetail">									<a href="javascript:;" class="amountType first active" data-unit="2">元</a>									<a href="javascript:;" class="amountType" data-unit="0.2">角</a>									<a href="javascript:;" class="amountType" data-unit="0.02">分</a>									<a href="javascript:;" class="amountType last" data-unit="0.002">厘</a>								</label>								<label class="changeMultDetail">									<span>倍数</span>									<div class="inputSelectDiv">										<a href="javascript:;" class="reduceMult" onclick="reduceMult()"></a>										<input type="text" class="labelCond" id="betPrice" value="1"/>										<a href="javascript:;" class="addMult" onclick="addMult()"></a>									</div>								</label>								<label class="betsInfoTypeList">									<c:choose>										<c:when test="${user.rebateRatio>0}">											<select class="labelCond betsInfoType" id="bounsType">												<option value="0"><fmt:formatNumber groupingUsed="false"																					value="${(bonusGroup.bonusRatio+user.rebateRatio) *20}"/>-0%(高奖)												</option>											</select>										</c:when>										<c:otherwise>											<select class="labelCond betsInfoType" id="bounsType">												<option value="0"><fmt:formatNumber groupingUsed="false"																					value="${(bonusGroup.bonusRatio+user.rebateRatio) *20}"/>												</option>											</select>										</c:otherwise>									</c:choose>								</label>								<div class="traceNum">									<span>连续开奖:</span>									<select class="labelCond" id="traceNum">										<option value="1">1</option>										<option value="2">2</option>										<option value="3">3</option>										<option value="4">4</option>										<option value="5">5</option>									</select>									<span>次</span>								</div>							</div>						</div>						<!--彩种追号信息-->						<div class="appNumBox">							<table class="appNumList">								<thead>								<tr>									<th width="120">玩法</th>									<th>号码</th>									<th width="40">模式</th>									<th width="80">注数</th>									<th width="80">倍数</th>									<th width="100">金额</th>									<th width="40" class="appNumClear">清空</th>								</tr>								</thead>								<tbody id="selectBetPanle"></tbody>							</table>						</div>					</div>				</div>			</div>		</div>	</div></div><!-- 左侧浮动：所有游戏 --><%@include file="/WEB-INF/jsp/home/left.jsp" %></body></html>