<%@page contentType="text/html" pageEncoding="UTF-8" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><!-- 导航 --><div class="leftListArea" id="rechargeNavUl">	<div class="leftListDetail">		<a class="leftListBigItem rechargeA" href="javascript:;"><i></i>充值</a>		<div class="leftListItem" style="display:none;">			<c:if test="${hadBankLevel}">				<a class="nav" id="rechargeMoneyBank" href="/rechargeNew/rechargeMoney?rechargeWay=5">银行转账</a>				<a class="nav" id="rechargeMoneyWeixin" href="/rechargeNew/rechargeMoney?rechargeWay=1">微信支付A</a>				<!--<a class="nav" id="rechargeMoneyWeixinB" href="/rechargeNew/rechargeMoney?rechargeWay=11">微信支付B</a>-->				<%--<a class="nav" id="rechargeMoneyAlipay" href="/rechargeNew/rechargeMoney?rechargeWay=2">支付宝B</a>--%>				<a class="nav" id="rechargeMoneyQQ" href="/recharge/rechargePay?type=qq">QQ钱包</a>				<%--<a class="nav" id="rechargeMoneyOnline" href="/recharge/rechargePay">网银支付</a>--%>			</c:if>		</div>	</div>	<div class="leftListDetail">		<a class="leftListBigItem depositA" id="navDeposit" href="javascript:;"		   onclick="userWithdraw()"><i></i>提现</a>	</div></div><script type="text/javascript">    $(function () {        var href = window.location.href;        console.log(href)        if ($("#rechargeNavUl .active").length == 0) {            if (href.indexOf('deposit') > 0 && href.indexOf('Outstanding') < 0) {                $("#navDeposit").addClass("active");            } else if (href.indexOf('rechargeMoneyBank') > 0) {                $('.leftListBigItem').eq(0).click();                $("#rechargeMoneyBank").addClass("active");            }  else if (href.indexOf('rechargeMoneyWeixinB') > 0) {                $('.leftListBigItem').eq(0).click();                $("#rechargeMoneyWeixinB").addClass("active");            } else if (href.indexOf('rechargeMoneyWeixin') > 0) {                $('.leftListBigItem').eq(0).click();                $("#rechargeMoneyWeixin").addClass("active");            }            else if (href.indexOf('rechargeMoneyAlipay') > 0) {                $('.leftListBigItem').eq(0).click();                $("#rechargeMoneyAlipay").addClass("active");            } else if (href.indexOf('rechargeMoneyHuabei') > 0) {                $('.leftListBigItem').eq(0).click();                $("#rechargeMoneyHuabei").addClass("active");            } else if (href.indexOf('rechargeMoneyCreditc') > 0) {                $('.leftListBigItem').eq(0).click();                $("#rechargeMoneyCreditc").addClass("active");            } else if (href.indexOf('rechargeMoney') > 0 || (href.indexOf('rechargeFinish') > 0)) {                $('.leftListBigItem').eq(0).click();                $("#nvaRechargeMoney").addClass("active");            } else if (href.indexOf('rechargeLower') > 0) {                $('.leftListBigItem').eq(0).click();                $("#nvaRechargeLower").addClass("active");            }            else if (href.indexOf('rechargePay') > 0 && href.indexOf('type=qq') > 0) {                $('.leftListBigItem').eq(0).click();                $("#rechargeMoneyQQ").addClass("active");            }            else if (href.indexOf('rechargePay') > 0 && href.indexOf('type=ali') > 0) {                $('.leftListBigItem').eq(0).click();                $("#rechargeMoneyAliScan").addClass("active");            }            else if (href.indexOf('rechargePay') > 0) {                $('.leftListBigItem').eq(0).click();                $("#rechargeMoneyOnline").addClass("active");            }            else if (href.indexOf('Outstanding') > 0) {                $('.outstandingA').addClass("active").click();                if (href.indexOf('rechargeOutstanding') > 0) {                    $('.rechNav').addClass("active");                } else {                    $('.depoNav').addClass("active");                }            }        }        else {            $('.leftListBigItem').eq(0).click();        }    });</script>