<%@page contentType="text/html" pageEncoding="UTF-8" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><c:if test="${null != rechargeList && rechargeList.size() > 0 }">	<c:forEach items="${rechargeList }" var="re" varStatus="st">		<div class="tableBoxNone" id="unRechargeDiv_${st.index }" style="display: none"><!-- class为弹窗名 -->			<!-- 弹窗内容 -->			<p class="fontColorRed">对不起，您尚有一笔充值申请未完成，请完成后再发起</p>			<table border="0" cellspacing="10" cellpadding="0">				<tbody>				<tr>					<td align="right">充值银行：</td>					<td align="left">${re.bankName }						<span style="margin-left: 80px">			        						<c:if test="${re.rechargeType == 0 }">												<a target="_blank" href="${re.receiveLink }">登录网上银行付款</a>											</c:if>			        						<c:if test="${re.rechargeType == 1 }">												<c:if test="${isQrCode }">													<a href="/recharge/rechargePayQrcodeResult?rechargeId=${re.id }">登录网上银行付款</a>												</c:if>												<c:if test="${!isQrCode }">													<a target="_blank"													   href="/recharge/rechargePayForward?rechargeId=${re.id }">登录网上银行付款</a>												</c:if>											</c:if>			        					</span>					</td>				</tr>				<tr>					<td align="right">充值金额：</td>					<td align="left">${re.amount } 元</td>				</tr>				<tr>					<td align="right">收款银行：</td>					<td align="left">${re.receiveBankName }</td>				</tr>				<c:if test="${re.rechargeType == 0 }">					<tr>						<td align="right">收款账户名：</td>						<td align="left">${re.receiveNiceName }</td>					</tr>					<tr>						<td align="right">收款卡号：</td>						<td align="left">${re.receiveCard }</td>					</tr>					<tr>						<td align="right">开户银行名称：</td>						<td align="left">${re.receiveAddress }</td>					</tr>				</c:if>				<tr>					<td align="right">附言：</td>					<td align="left">${re.traceId }</td>				</tr>				</tbody>			</table>			<!-- 弹窗按钮 -->			<div class="dialogBtn">				<a href="javascript:;" onclick="rechargeCancel('${re.id }');" class="btn">撤销申请</a>			</div>			<p>* 如您已完成付款，请勿撤销，我们将尽快为您处理。</p>		</div>		<div id="tableBoxSuccess" style="display: none">			<p class="lottTipLayerTitle">温馨提示</p>			<div class='lottTipBox'>				<p class='msg'>					<i></i>					<span class='content fontColorRed'>您的充值申请已撤销成功！</span>				</p>			</div>			<div class='lottTipBtn'>				<a href='javascript:window.location.reload(true);' class='btn closeBtn'>继续充值</a>			</div>		</div>	</c:forEach></c:if><script type="text/javascript">	function rechargeCancel(id) {		ajaxObject('/recharge/rechargeCancel?id=' + id, "POST", function (data) {			if (data == 1) {				layer.closeAll();				layer.open({					type: 1,					skin: 'lottTipLayer',					shift: 5,					area: ['480px', '260px'],					title: false,					content: $("#tableBoxSuccess"),					cancel: function () {						layer.closeAll();						window.location.reload(true);					}				});			} else {				window.location.reload(true);			}		});	}	function checkHadUnRecharge() {		var unRecharge = $("#unRechargeDiv_0")[0];		if (unRecharge) {			layer.closeAll();			layer.open({				type: 1,				skin: 'unRechargeLayer',				shift: 5,				area: '480px',				title: '温馨提示',				content: $("#unRechargeDiv_" + 0)			});			return false;		}		return true;	}</script>