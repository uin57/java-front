<%@page contentType="text/html" pageEncoding="UTF-8" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><input type="hidden" id="pageCountFinance" value="${p.rowCount }"/><table class="userListTable" id="financeListTable">	<tr class="listHeader">		<th width="5%">序号</th>		<th width="10%">帐变用户</th>		<th width="10%">帐变编号</th>		<th width="8%">帐变类型</th>		<th width="12%">帐变时间</th>		<th width="10%">收入</th>		<th width="10%">支出</th>		<th width="10%">用户余额</th>		<th width="20%">备注</th>		<th width="5%">状态</th>	</tr>	<c:forEach items="${financeChangeList }" var="financeChange" varStatus="st">		<tr class="listDetail">			<td>${st.index + p.startIndex + 1 }</td>			<td>${financeChange.changeUser }</td>			<td id="tdFinanceId_${st.index + 1 }">${financeChange.financeId }</td>			<td>				<c:forEach items="${accountChangeTypeList }" var="fct">					<c:if test="${fct.id == financeChange.accountChangeTypeId}">${fct.name }</c:if>				</c:forEach>			</td>			<td><fmt:formatDate value='${financeChange.changeTime}' pattern="yyyy-MM-dd HH:mm:ss"/></td>			<td id="tdChangeAmount_in_${st.index + 1 }">${financeChange.changeAmount >= 0 ? financeChange.changeAmount : 0 }</td>			<td id="tdChangeAmount_ot_${st.index + 1 }">${financeChange.changeAmount < 0 ? financeChange.changeAmount : 0 }</td>			<td>${financeChange.balance }</td>			<td>${financeChange.remark }</td>			<td id="tdStatus_${financeChange.id }">				<c:if test="${financeChange.status == 0 }">未处理</c:if>				<c:if test="${financeChange.status == 1 }">拒绝</c:if>				<c:if test="${financeChange.status == 2 }">完成</c:if>				<c:if test="${financeChange.status == 3 }">已过期</c:if>				<c:if test="${financeChange.status == 4 }">已撤销</c:if>				<c:if test="${financeChange.status == 5 }">正在处理</c:if>				<c:if test="${financeChange.status == 6 }">审核中</c:if>				<c:if test="${financeChange.status == 7 }">审核通过</c:if>				<c:if test="${financeChange.status == 8 }">审核不通过</c:if>				<c:if test="${financeChange.status == 99 }">挂起</c:if>			</td>		</tr>	</c:forEach>	<c:if test="${financeChangeList != null && financeChangeList.size() > 0 }">		<tr class="listDetail">			<td colspan="5"></td>			<td class="fontColorTheme"><span id="tdIn"></span></td>			<td class="fontColorTheme"><span id="tdOut"></span></td>			<td class="fontColorTheme" colspan="4">				<span class="fontColorBlack">帐变金额：</span>				<span id="tdAll"></span>			</td>		</tr>	</c:if></table><c:if test="${financeChangeList == null || financeChangeList.size() == 0 }">	<!-- 没有消息 -->	<div class="nullMsg" id="nullMsg">		<i class="nullMsgImg"></i>		<span class="fontColorGray">没有符合条件的记录，请更改查询条件！</span>	</div></c:if><script type="text/javascript">	$(function () {		var o = 0, i = 0, a = 0;		// 统计		$("#financeListTable td[id^='tdChangeAmount_']").each(function () {			var m = parseFloat($(this).html());			if (m < 0) {				o = Math.add(o, m);			} else {				i = Math.add(i, m);			}			a = Math.add(a, m);		});		$("#tdOut").html(o);		$("#tdIn").html(i);		$("#tdAll").html(a);	});</script>