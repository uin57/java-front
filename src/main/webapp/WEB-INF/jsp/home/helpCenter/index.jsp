<%@page contentType="text/html" pageEncoding="UTF-8" %><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt" %><!DOCTYPE html><html lang="en"><head>	<meta charset="UTF-8">	<title>帮助中心</title>	<link rel="stylesheet" href="<c:url value='/res/home/css/wk-helpCenter.css?ver=${VIEW_VERSION}'/>"/>	<link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">	<script type="text/javascript" src="<c:url value='/res/home/js/helpCenter/index.js?ver=${VIEW_VERSION}'/>"></script>	<script type="text/javascript">		var id = '${id }';		$(function () {			if (id) {				$(".leftListArea a[data-id='" + id + "']").click();			}		});	</script></head><body><!-- 帮助中心 --><div class="areaBigContainer mainWidth" id="helpCenterArea">	<div class="containerFlex">		<!-- 帮助中心-列表 -->		<div class="leftListArea">			<c:forEach items="${articleModels}" var="a" varStatus="status">				<div class="leftListDetail">					<c:if test="${a.type==0}">						<h2 class="leftListBigItem"> ${a.articleGroupTitle}</h2>						<div class="leftListItem" style="display:none;">							<c:forEach items="${a.atrArticles}" var="b" varStatus="status">								<c:if test="${b.status==0}">									<a href='javascript:;' class="nav" data-id="${b.id}"> ${b.title}</a>								</c:if>							</c:forEach>						</div>					</c:if>				</div>			</c:forEach>		</div>		<!-- 帮助中心内容 -->		<div class="rightArea">			<!-- 帮助中心刚进入展示区域 -->			<div class="indexBox">				<div class="helpCenterIndex-l">					<!-- 自主服务 -->					<div class="autoServicesBox">						<h3>自主服务</h3>						<div class="autoServices">							<a href="/user/index?tabId=modLoginPwd" class="autoServiceDetail">								<i class="lock-white"></i>								<span>修改登录密码</span>							</a>							<a href="/user/index?tabId=modAccountPwd" class="autoServiceDetail">								<i class="money-white"></i>								<span>修改安全密码</span>							</a>							<a href="/user/index?tabId=personInfo" class="autoServiceDetail">								<i class="qq-white"></i>								<span>修改QQ</span>							</a>							<a href="/user/index?tabId=personInfo" class="autoServiceDetail">								<i class="msg-white"></i>								<span>修改邮箱</span>							</a>						</div>					</div>					<!-- 最受关注的问题 -->					<div class="focusQuestionsBox" id="focusQuestionsBox">						<c:forEach items="${articleModels}" var="a" varStatus="status">							<c:if test="${a.type==1}">								<h3>${a.articleGroupTitle}</h3>								<ol class="focusQuestions">									<c:forEach items="${a.atrArticles}" var="b" varStatus="status">										<li class="focusQuestionDetail"><a href='javascript:;' class='textUnderline'																		   data-id="${b.id}">${b.title}</a></li>									</c:forEach>								</ol>							</c:if>						</c:forEach>					</div>				</div>				<div class="helpCenterIndex-r">					<h3>在线客服</h3>					<div class="helpOnlineService">						<a href="${sysService.link }" class="btn" target="_blank">点击开始</a>					</div>					<span>以下是我们的客服人员，我们绝对不会以任何形式向您索要您的账号和密码，请妥善保管好您的账号信息！</span>					<div class="onlineServicePersons">						<label for="#">							<img src="<c:url value='/res/home/images/helpCenter/servicePersonIcon1.png'/>" alt="春兰"/>							<span>Candy</span>						</label>						<label for="#">							<img src="<c:url value='/res/home/images/helpCenter/servicePersonIcon2.png'/>" alt="夏荷"/>							<span>Linda</span>						</label>						<label for="#">							<img src="<c:url value='/res/home/images/helpCenter/servicePersonIcon3.png'/>" alt="秋菊"/>							<span>大山</span>						</label>						<label for="#">							<img src="<c:url value='/res/home/images/helpCenter/servicePersonIcon4.png'/>" alt="冬梅"/>							<span>婷婷</span>						</label>						<label for="#">							<img src="<c:url value='/res/home/images/helpCenter/servicePersonIcon5.png'/>" alt="春雷"/>							<span>美美</span>						</label>						<%--<label for="#">--%>						<%--<img src="<c:url value='/res/home/images/helpCenter/servicePersonIcon6.png'/>" alt="夏雨" />--%>						<%--<span>夏雨</span>--%>						<%--</label>--%>						<%--<label for="#">--%>						<%--<img src="<c:url value='/res/home/images/helpCenter/servicePersonIcon7.png'/>" alt="秋霜" />--%>						<%--<span>秋霜</span>--%>						<%--</label>--%>						<%--<label for="#">--%>						<%--<img src="<c:url value='/res/home/images/helpCenter/servicePersonIcon8.png'/>" alt="冬雪" />--%>						<%--<span>冬雪</span>--%>						<%--</label>--%>					</div>				</div>			</div>			<!-- 帮助中心-通用详情 -->			<div class="helpCenterCommonDetail" style="display:none;" id="helpCenterCommonDetail">				<div class="helpCenterCommonTitle">					<h3 id="helpCenterCommonTitle">如何在大順平台进行购彩？</h3>					<span id="helpCenterCommonTime">2015-12-21 18:26</span>				</div>				<div class="helpCenterCommonContent" id="helpCenterCommonContent"></div>				<div class="helpCenterCommonBtn">					<p>如果以上内容无法解决您的问题，请联系我们的客服</p>					<a href="${sysService.link }" class="btn helpCenterServiceOL" target="_blank">在线咨询</a>				</div>			</div>		</div>	</div>	<%--<i class="shadow"></i>--%>	<%--<i class="shadow2"></i>--%>	<%--<i class="shadow3"></i>--%></div></body></html>