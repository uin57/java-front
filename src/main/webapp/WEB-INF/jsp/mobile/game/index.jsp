<%@page contentType="text/html" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <meta http-equiv="Cache-Control" content="no-siteapp" />    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0 user-scalable=no">    <meta content="yes" name="apple-mobile-web-app-capable">	<meta content="black" name="apple-mobile-web-app-status-bar-style">	<meta content="telephone=no" name="format-detection">	<meta content="email=no" name="format-detection">    <title>投注记录</title>  <%--   <script type="text/javascript" src="<c:url value='/res/mobile/js/game/index.js?ver=${VIEW_VERSION}'/>"></script> --%>    <link rel="stylesheet" href="/res/mobile/css/hs-mobile.css" /></head><body class="bodyColorEEE"><%@include file="/WEB-INF/jsp/mobile/left.jsp" %><!-- 投注记录 --><div class="betsListArea generalPlatArea">    <div class="mainNav">        <a href="javascript:;" class="mainMenuBtn">            <img src="/res/mobile/images/mainMenuIcon.png" class="mainMenuIcon" />        </a>        <span class="headerTitle">投注记录</span>    </div>    <div class="container">        <div class="betsListBox">            <table class="betsListTable" id="gameRecordListTable">                <thead>                    <tr class="tableHeader">                        <th width="90">时间</th>                        <th>彩种</th>                        <th>金额</th>                        <th>状态</th>                    </tr>                </thead>                <tbody id="gameRecordHeader">                </tbody>            </table>        </div>    </div></div><script type="text/javascript" src="<c:url value='/res/home/js/spinner.js?ver=${VIEW_VERSION}'/>"></script><script type="text/javascript">$(document).ready(function(){	var args = "&account=''&includeChildren=1&seasonId=''&lotteryId=''&playerId=''&startTime=''&endTime=''";	ajaxExt({		url:'/game/list',		method:"get",		data: args,		callback:function(data) {		 $("#gameRecordListTable tr:gt(0)").each(function(){				$(this).remove();			}); 						var rows = data.rows;			for(var i = 0;i < rows.length;i++){				var tr = $("<tr></tr>");				$(tr).append("< <td class='fontSizeRem075'>"+rows[i].createTime+"</td>");				$(tr).append(" <td class='sonBreak'><span>"+rows[i].lotteryName+"</span><span class='fontSizeRem075'>"+rows[i].playName+"</span></td>");				$(tr).append("<td class='fontColorRed'>"+rows[i].amount+"</td>");				switch(rows[i].status){				 case 0:  $(tr).append("<td class='sonBreak'><span>等待开奖</span><a  class='cancelOrder fontColorTheme' href='/lotts/"+rows[i].lotteryId+'/cancelOrder?ids='+ rows[i].id + "'>撤单</a></td>");break;				 case 1: $(tr).append("<td class='sonBreak fontColorRed'><span>已中奖</span><span>"+rows[i].win+"</span>元</td>");break;				 case 2: $(tr).append("<td class='sonBreak'><span>未中奖</span></td>");break;				 case 3:  $(tr).append("<td class='sonBreak'><span>未开始</span><a class='cancelOrder fontColorTheme' id='cancelOrderByTrace' href='/lotts/"+rows[i].lotteryId+'/cancelOrder?ids='+ rows[i].id + "'>撤单</a></td>");break;				 case 4: $(tr).append("<td class='sonBreak'><span>个人撤单</span></td>");break;				 case 5: $(tr).append("<td class='sonBreak'><span>系统撤单</span></td>");break;				 case 6:  $(tr).append("<td class='sonBreak'><span>未开奖</span></td>");break;				 case 7:  $(tr).append("<td class='sonBreak'><span>恶意注单</span></td>");break;				 case 8:  $(tr).append("<td class='sonBreak'><span>暂停</span></td>");break;				 case 9:  $(tr).append("<td class='sonBreak'><span>追中撤单</span></td>");break;				 }				$('#gameRecordHeader').append(tr);			}		}	})});//撤销投注$(document).on("click","a.cancelOrder",function(ev){	var a = $(this);	var url = a.attr("href");	ajaxExt({		url:url,		callback:function(rel){			$.alert({text: "撤销成功", icon: 'ok'});			a.siblings().text("个人撤单");			a.remove();		}	});	ev.stopPropagation();	ev.preventDefault();	return false;});</script></body></html>