<%@page contentType="text/html" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><!DOCTYPE html><html lang="en"><head><meta charset="UTF-8"><meta http-equiv="Cache-Control" content="no-siteapp" /><meta name="viewport"	content="width=device-width, initial-scale=1.0, maximum-scale=1.0 user-scalable=no"><meta content="yes" name="apple-mobile-web-app-capable"><meta content="black" name="apple-mobile-web-app-status-bar-style"><meta content="telephone=no" name="format-detection"><meta content="email=no" name="format-detection"><title>用户列表</title><link rel="stylesheet" href="<c:url value='/res/mobile/css/hs-mobile.css?ver=${VIEW_VERSION}'/>" /></head><body class="bodyColorEEE"><%@include file="/WEB-INF/jsp/mobile/left.jsp" %><!-- 用户列表 --><div class="generalPlatArea userListArea">	<div class="mainNav">		<a href="javascript:;" class="mainMenuBtn">			<img src="/res/mobile/images/mainMenuIcon.png" class="mainMenuIcon" />		</a>		<span class="headerTitle">用户列表</span>		<a href="/user/list" class="returnBtn" style="display:none;">			<img src="/res/mobile/images/returnIcon.png" />			<span>返回</span>		</a>	</div>	<div class="container">		<!-- 用户列表页 -->		<div class="userListBox">			<form class="userListSearchBar">				<label>					<input type="text" id="userName" name="account" class="labelCond" placeholder="输入用户名查询"  />					<input type="hidden" id="userNameH" a='0' />					<i class="searchIcon"></i>				</label>				<button type="button" class="btn searchUser btnRed" onclick='search()'>搜索</button>			</form>			<div class="userListContent">				<input type="hidden" id="userRebateRatios" value="${userRebateRatio}" />				<input type="hidden" id="userSafeWord" value="${user.safePassword == null ? '' : 'have'}" />				<input type="hidden" id="accountRecharge" value="${user.accountRecharge}" />			    <table class="userListTable">					<tr class="listHeader">						<th>用户名</th>						<th>返点</th>						<th>余额</th>					</tr>					<tbody id="content">						<c:import url="listRecordBody.jsp"></c:import>					</tbody>				</table>		     <div class="loadMoreBox">            	<a href="javascript:loadMore();" class="loadMore" id="loadMore">点击加载更多</a>			</div>			</div>		</div>		<!-- 返点调配页 -->		<div class="userListSetBox setReservePointBox" id="setReservePointBox" style="display:none;">			<table class="userReservePointInfo">				<tr class="listDetail">					<td>用户名：</td>					<td id="rebatePerson"></td>				</tr>				<tr class="listDetail">					<td>昵称：</td>					<td id="rebateNickName"></td>				</tr>				<tr class="listDetail">					<td>自身返点：</td>					<td id="selfRebateRatio"></td>				</tr>			</table>			<form class="setReservePointForm">				<p class="setPointDetail">调整返点（向上）</p>				<p class="setPointDetail">					<input type="hidden" id="smart" name="smart" />					<input type="hidden" id="bigger" name="bigger"/>					<span>设置为：</span>					<a href="javascript:;" class="setPoint reduce" onclick="reduceRebate()">-</a>					<input type="text" id="UserQuota" class="labelCond" />					<a href="javascript:;" class="setPoint add" onclick="addRebate()">+</a>				</p>				<button type="button" class="btn btnRed" onclick="doAdjustQuota()">完成</button>			</form>		</div>		<!-- 配额分配页 -->		<div class="userListSetBox setQuotaAllotBox" id="setQuotaAllotBox" style="display:none;">			<table class="userQuotaAllotInfo">				<tr class="listDetail">					<td>用户名：</td>					<td id="quotaUserName"></td>				</tr>				<tr class="listDetail">					<td>自身返点：</td>					<td id="myRebateRatio" class="fontColorRed"></td>				</tr>			</table>			<form class="setQuotaAllotForm" id="quotaDistTable">				<!-- <table class="quotaDistTable" id="quotaDistTable">				    <tr class="listDetail">						<td colspan="2" class="quotaIntro">							<ul class="quotaNum">								<li>									<span>用户名：</span> <span class="fontColorRed" id="quotaUserName"></span>									<span>返点：</span> <span class="fontColorRed" id="myRebateRatio"></span>								</li>							</ul>						</td>					</tr>				</table> -->			</form>			<button type="button" class="btn btnRed" onclick="doSetUserQuota(this)">提交</button>		</div>		<!-- 下级充值页 -->		<div class="setSubordRechBox" id="setSubordRechBox" style="display:none;">			<div class="listForm">				<form  action="" id="rechargeLowerForm" method="post" onsubmit="return checkForm();">				    <input type="hidden" id="rechargeType" name="rechargeType" value="2" />					<div class="listDetail">						<p class="detailTitle fontColorYellow">							<i class="detailTitleLeft"></i>							<span>目标用户</span>						</p>						<input type="text" id="targetUser" class="labelCond" name="targetUser" readonly="true"/>					</div>					<div class="listDetail">						<p class="detailTitle fontColorYellow">							<i class="detailTitleLeft"></i>							<span>充值金额</span>						</p>						<input type="tel" class="labelCond" id="chargeamount" name="chargeamount" onkeyup="checkMoney(this)" placeholder="请输入充值金额" />					</div>					<div class="listDetail">						<p class="detailTitle fontColorYellow">							<i class="detailTitleLeft"></i>							<span>安全密码</span>						</p>						<input type="password" class="labelCond" id="sourceUserSafePassword" name="sourceUserSafePassword" maxLength=16 placeholder="请输入8~16位安全密码" />					</div>					<div class="listDetail">						<p class="detailTitle">							<i class="detailTitleLeft"></i>							<span>充值形式</span>											</p>						<select class="labelCond" id="rechargeStyle" name="rechargeStyle" data-id='rechargeStyle0' >	                        <option value="0" data-id='rechargeType0'>帐变形式（点击选择）</option>	                        <option value="1" data-id='rechargeType1'>工资（影响盈亏）</option>	                        <option value="2" data-id='rechargeType2'>团队活动（影响盈亏）</option>	                        <option value="3" data-id='rechargeType3'>周期分红（无影响）</option>	                        <option value="4" data-id='rechargeType4'>下级充值（无影响）</option>						</select>													</div>										<div class="listDetail purposeArea">						<div id="rechargeStyle0">							<p class="contentList">								<span class="contentL titleBold">注意事项：</span>								<span class="contentR">转点功能是您给下级进行转账的操作，请严格阅读注意事项！！</span>							</p>							<p class="contentList">								<span class="contentL titleBold">工资：</span>								<span class="contentR">该帐变为等同于发放工资给下级，会影响盈亏报表金额；</span>							</p>							<p class="contentList">								<span class="contentL titleBold">团队活动：</span>								<span class="contentR">发放活动金额给下级，会影响盈亏报表金额；</span>							</p>							<p class="contentList">								<span class="contentL titleBold">周期分红：</span>								<span class="contentR">发送分红给下级，不影响盈亏报表金额；</span>							</p>							<p class="contentList">								<span class="contentL titleBold">下级充值：</span>								<span class="contentR">帮下级充值，下级会增加相对应充值金额的所需出款消费量。</span>							</p>						</div>						<div id="rechargeStyle1">							<p class="contentList">								<span class="contentL titleBold">工资：</span>								<span class="contentR">该帐变为等同于发放工资给下级，会影响盈亏报表金额；</span>							</p>						</div>						<div id="rechargeStyle2">							<p class="contentList">								<span class="contentL titleBold">团队活动：</span>								<span class="contentR">发放活动金额给下级，会影响盈亏报表金额；</span>							</p>						</div>						<div id="rechargeStyle3">							<p class="contentList">								<span class="contentL titleBold">周期分红：</span>								<span class="contentR">发送分红给下级，不影响盈亏报表金额；</span>							</p>						</div>						<div id="rechargeStyle4">							<div class="listDetail">								<p class="detailTitle">									<i class="detailTitleLeft"></i>									<span>备注用途</span>								</p>								<select class="labelCond" id="remark" onchange="selectRemark(this.value)">									<option value="上级代充">上级代充</option>									<option value="借款">借款</option>									<option value="红包">红包</option>									<option value="0" selected="selected">其他原因</option>								</select>							</div>															<div class="listDetail">								<input type="text" class="labelCond" id="remarkOtherReasion"  maxLength=5 placeholder="不得超过5个字"  />							</div>							<div class="listDetail">								<p class="contentList">									<span class="contentL titleBold">下级充值：</span>									<span class="contentR">帮下级充值，下级会增加相对应充值金额的所需出款消费量。</span>								</p>							</div>													</div>					</div>				</form>			</div>			<div class="btnGroup">				<a href="javascript:void(0);" class="btn btnRed" onclick="doDownRecharge()">提交</a>			</div>		</div>	</div></div><!-- 弹窗、遮罩 --><div id="mask" class="mask" style="display: none;"></div><div class="dialogArea" id="dialogBox"></div><script type="text/javascript" src="<c:url value='/res/mobile/js/jquery.dialog.js?ver=${VIEW_VERSION}'/>"></script><script type="text/javascript" src="<c:url value='/res/home/js/spinner.js?ver=${VIEW_VERSION}'/>"></script><script type="text/javascript" src="<c:url value='/res/mobile/js/user/userList.js?ver=${VIEW_VERSION}'/>"></script><script type="text/javascript">var specicalStr = "${specicalStr }";function checkHadSpecialChar(text) {	for (var i in specicalStr) {		if (text.indexOf(specicalStr[i]) >= 0) {			return true;		}	}	return false;}</script></body></html>